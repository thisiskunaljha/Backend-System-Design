{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Django Server",
            "type": "python",
            "request": "launch",
            "manage": true,
            "program": "${workspaceFolder}/community/manage.py",
            "args": ["runserver"],
            "django": true,
            "jinja": true,
            "justMyCode": true
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}
